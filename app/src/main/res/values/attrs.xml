<?xml version="1.0" encoding="utf-8"?>
<resources>
	<attr name="icon_size"></attr>

	<declare-styleable name="SplitterLayout">
		<attr name="separator" format="float"/>
		<attr name="minSeparator" format="float"/>
		<attr name="maxSeparator" format="float"/>
		<attr name="touchSeparatorEnabled" format="boolean"/>
		<attr name="autoMoveSeparator" format="boolean"/>
		<attr name="autoChangeOrientation" format="boolean"/>
	</declare-styleable>
	
	<declare-styleable name="Editor">
		<attr name="textColor" format="color"/>
		<attr name="textSize" format="dimension"/>
		<attr name="cursorColor" format="color"/>
		<attr name="selectionColor" format="color"/>
		<attr name="scrollBarColor" format="color"/>
		<attr name="highlightScrollBarColor" format="color"/>
		<attr name="highlightLineColor" format="color"/>
		<attr name="lineNumberColor" format="color"/>
		<attr name="lineSpacing" format="float"/>
		<attr name="TabSize" format="integer"/>
	</declare-styleable>
	
</resources>
